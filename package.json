    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "power-assert": "^1.4.4",
    "promise-retry": "^1.1.1",
    "serialize-error": "^2.1.0",
    "shelljs": "^0.7.8",
    "stream-spigot": "^3.0.6",
    "tmp-promise": "^1.0.3",
    "utf8": "^2.1.2",
