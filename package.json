    "base-64": "^0.1.0",
    "body-parser": "^1.18.2",
    "child-process-promise": "^2.2.1",
    "config": "^1.26.2",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "continuation-local-storage": "^3.2.0",
