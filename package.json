    "github": "^11.0.0",
    "glob-stream": "^6.1.0",
    "graphql": "^0.10.0",
    "graphql-tag": "^2.4.2",
    "inquirer": "^3.3.0",
    "isomorphic-fetch": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "lru_map": "^0.3.3",
    "memorystore": "^1.5.1",
