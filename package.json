    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/base-64": "^0.1.2",
    "@types/body-parser": "^1.16.5",
    "@types/config": "0.0.33",
    "@types/connect-ensure-login": "^0.1.3",
    "@types/connect-flash": "0.0.33",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/fs-extra": "^4.0.2",
    "@types/graphql": "^0.11.5",
    "@types/inquirer": "0.0.35",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.77",
    "@types/minimatch": "^3.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.34",
    "@types/passport": "^0.3.4",
    "@types/passport-github": "^1.1.1",
    "@types/passport-http": "^0.3.2",
    "@types/passport-http-bearer": "^1.0.30",
    "@types/power-assert": "^1.4.29",
    "@types/shelljs": "^0.7.4",
    "@types/utf8": "^2.1.6",
    "@types/winston": "^2.3.6",
    "@types/yargs": "^8.0.2",
    "espower-typescript": "^8.0.2",
    "mocha": "^4.0.1",
    "supervisor": "^0.12.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.4.3",
    "typedoc": "^0.9.0",
    "typescript": "2.5.3",
    "typescript-formatter": "^6.0.0"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autoclient": "nodemon --inspect -e js build/src/start.client.js",
    "autotest": "supervisor -q -n exit -e ts -x npm -- run test",
    "benchmark": "mocha --prof --compilers ts:espower-typescript/guess \"test/**/*Benchmark.ts\"; node --prof-process isolate-* > profile.txt",
    "build": "npm run lint && npm run compile && npm test",
    "clean": "npm run clean-js ; rm -rf *-v8.log profile.txt build",
    "clean-js": "find src test -type f -name '*.js' -print0 | xargs -0 rm -f",
    "cli": "npm run compile && node $NODE_DEBUG_OPTION build/src/start.cli.js",
    "client": "npm run compile && node $NODE_DEBUG_OPTION --trace-warnings build/src/start.client.js --open=true",
    "compile": "tsc -p . && cp src/graph/schema.cortex.json build/src/graph/schema.cortex.json",
    "distclean": "npm run clean ; rm -rf node_modules",
    "fmt": "tsfmt --replace",
    "git:info": "npm run compile && node $NODE_DEBUG_OPTION --trace-warnings build/src/git.info.js",
    "lint": "tslint --format verbose --project . --exclude '{build,node_modules}/**' '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --require espower-typescript/guess 'test/**/!(*atomist.config).ts' --exit",
    "testone": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.ts}\" --exit",
    "typedoc": "typedoc --mode modules --excludeExternals"
  },
  "bin": {
    "git-info": "./git.info.js",
    "atomist-cli": "./start.cli.js",
    "atomist-client": "./start.client.js",
    "atomist-config": "./atomist-config.js"
  },
  "engines": {
    "node": "8.5.x",
    "npm": "5.3.x"
  }
}
